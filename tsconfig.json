{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "public"],
  "exclude": ["node_modules"],
  "types": [
    "node",
    "react",
    "react-dom",
    "google.maps",
    "hoist-non-react-statics",
    "json5",
    "prop-types",
    "react-places-autocomplete",
    "react-transition-group",
    "use-sync-external-store",
    "yup"
  ]
}
